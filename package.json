{
  "name": "cowin-automation",
  "version": "1.0.0",
  "description": "CoWIN API poller, Webhook provider and Telegram automation",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --ignore *.json --exec babel-node --trace-warnings ./src/index.js",
    "build": "npx babel -d ./build/lib ./lib && npx babel -d ./build/src ./src -s",
    "clean": "rm -rf build && mkdir build",
    "clean-build": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-manager": "npx babel-node src/StartWebhookManager.js",
    "dev-server": "npx babel-node src/StartWebhookServer.js",
    "cowin-api-test": "npx babel-node src/CoWINApiTest.js",
    "appointment-test": "npx babel-node src/AppointmentUtilsTest.js",
    "telegram-bot": "npx babel-node ./build/src/StartTelegramBot.js",
    "serve": "node ./build/index.js",
    "start": "npm run telegram-bot"
  },
  "keywords": [
    "cowin",
    "webhook",
    "automation"
  ],
  "author": "Atmanand Nagpure",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.12.1",
    "nodemon": "^2.0.7",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@babel/node": "^7.14.2",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "telegraf": "^4.3.0"
  }
}
