{
  "name": "cowin-automation",
  "version": "1.0.0",
  "description": "CoWIN API poller, Webhook provider and Telegram automation",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --ignore *.json --exec babel-node --trace-warnings ./src/index.js",
    "build": "babel -d ./build ./src -s",
    "clean": "rm -rf build && mkdir build",
    "clean-build": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve",
    "serve": "node ./build/index.js"
  },
  "keywords": [
    "cowin",
    "webhook",
    "automation"
  ],
  "author": "Atmanand Nagpure",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  }
}
